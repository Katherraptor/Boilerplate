{
  "name": "wa-boilerplate",
  "version": "2.0.0",
  "description": "A Simple Boilerplate for Frontend Projects",
  "main": "index.html",
  "engines": {
    "node": ">=6.9"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "connect-livereload": "^0.5.4",
    "connect-ssi": "^1.1.0",
    "gulp": "^3.9.1",
    "jshint": "^2.9.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.0",
    "beepbeep": "^1.2.0",
    "connect-ssi": "^1.1.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.2",
    "gulp-beautify": "^2.0.1",
    "gulp-bower": "0.0.13",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^2.3.1",
    "gulp-cssnano": "^2.1.0",
    "gulp-eslint": "^3.0.1",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-jshint": "^2.0.0",
    "gulp-less": "^3.0.5",
    "gulp-livereload": "^3.8.1",
    "gulp-newer": "^1.1.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.2",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jquery": true
    },
    "parserOptions": {
      "ecmaVersion": "6",
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "warn"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://webadvanced.git.beanstalkapp.com/frontendboilerplate.git"
  },
